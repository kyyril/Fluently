{
    "name": "@fluently/api",
    "version": "0.0.0",
    "private": true,
    "main": "./src/server.ts",
    "scripts": {
        "dev": "prisma generate && tsx watch src/server.ts",
        "build": "prisma generate && tsc",
        "start": "node dist/src/server.js",
        "db:generate": "prisma generate",
        "db:push": "prisma db push",
        "db:migrate": "prisma migrate dev",
