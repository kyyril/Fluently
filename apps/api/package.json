        "compression": "^1.8.1",
        "cors": "^2.8.5",
        "dotenv": "^16.4.0",
        "express": "^4.18.0",
        "helmet": "^7.1.0",
        "ioredis": "^5.9.1",
        "jose": "^6.1.3",
        "jsonwebtoken": "^9.0.2",
        "jwks-rsa": "^3.2.0",
        "zod": "^3.22.0"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
